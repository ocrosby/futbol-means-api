{
  "name": "means-api",
  "version": "1.0.0",
  "title": "Means API",
  "description": "An API allowing users to efficiently enter match data.",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rimraf dist coverage",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "concurrently \"nodemon --exec npm run dev:start\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "start": "node .",
    "test": "jest",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "docker:build": "docker build -t means-api .",
    "docker:images": "docker images"
  },
  "repository": {
    "type": "git",
    "url": "something"
  },
  "keywords": [
    "futbol",
    "means"
  ],
  "author": "Omar Crosby",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.9.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^5.0.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.18.11",
    "@types/pug": "^2.0.6",
    "@types/shelljs": "^0.8.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "fs-extra": "^10.1.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm run dev:start"
  }
}
