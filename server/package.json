{
  "name": "means-api",
  "version": "1.0.0",
  "title": "Means API",
  "description": "An API allowing users to efficiently enter match data.",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "eslint .",
    "tsoa": "tsoa",
    "test": "jest",
    "tsoa:spec": "tsoa spec",
    "tsoa:routes": "tsoa routes",
    "tsoa:build": "npm-run-all tsoa:spec tsoa:routes",
    "tsc": "tsc",
    "compile": "npm-run-all clean lint tsoa:build tsc",
    "copy": "cp .env package*.json dist/",
    "install:dist": "cd dist/ && npm install --production",
    "build": "npm-run-all compile copy install:dist",
    "dev": "concurrently \"nodemon --config nodemon.json\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "npm run build && node dist/server.js",
    "mongo:start": "docker run -d --name mongo_dev -p 27017:27017 -e MONGO_INITDB_ROOT_USERNAME=root -e MONGO_INITDB_ROOT_PASSWORD=password mongo",
    "mongo:stop": "docker stop mongo_dev && docker container rm mongo_dev"
  },
  "repository": {
    "type": "git",
    "url": "something"
  },
  "keywords": [
    "futbol",
    "means"
  ],
  "author": "Omar Crosby",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "mongoose": "^6.9.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.1",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/inversify-binding-decorators": "^2.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^7.6.0",
    "eslint": "~8.22.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
