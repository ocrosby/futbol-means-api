{
  "name": "means-api",
  "version": "1.0.0",
  "title": "Means API",
  "description": "An API allowing users to efficiently enter match data.",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "eslint .",
    "tsoa": "tsoa",
    "test": "jest",
    "tsoa:spec": "tsoa spec",
    "tsoa:routes": "tsoa routes",
    "tsoa:build": "npm-run-all tsoa:spec tsoa:routes",
    "tsc": "tsc",
    "compile": "npm-run-all clean lint tsoa:build tsc",
    "copy": "cp .env package*.json dist/",
    "install:dist": "cd dist/ && npm install --production",
    "build": "npm-run-all compile copy install:dist",
    "dev0": "concurrently \"nodemon --config nodemon.json\" \"nodemon -x tsoa spec-and-routes\"",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "bstart": "npm run build && npm start",
    "mstart": "npm run mstop && docker run -d --name mongo_dev -p 27017:27017 -e MONGO_INITDB_ROOT_USERNAME=root -e MONGO_INITDB_ROOT_PASSWORD=password mongo",
    "mstop": "docker stop mongo_dev && docker container rm mongo_dev"
  },
  "repository": {
    "type": "git",
    "url": "something"
  },
  "keywords": [
    "futbol",
    "means"
  ],
  "author": "Omar Crosby",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.1",
    "tsoa": "^5.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^2.0.1",
    "@types/inversify-binding-decorators": "^2.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/passport-local-mongoose": "^6.1.1",
    "@types/pug": "^2.0.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^7.6.0",
    "eslint": "~8.22.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
