{
  "name": "means-api",
  "version": "1.0.0",
  "title": "Means API",
  "description": "An API allowing users to efficiently enter match data.",
  "main": "dist/server.js",
  "scripts": {
    "build": "make dist",
    "dev": "concurrently \"nodemon --config nodemon.json\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "node dist/server.js",
    "mongo": "docker run -d --name mongo_dev -p 27018:27017 -e MONGO_INITDB_ROOT_USERNAME=root -e MONGO_INITDB_ROOT_PASSWORD=password mongo"
  },
  "repository": {
    "type": "git",
    "url": "something"
  },
  "keywords": [
    "futbol",
    "means"
  ],
  "author": "Omar Crosby",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "mongoose": "^6.9.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.1",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/inversify-binding-decorators": "^2.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
