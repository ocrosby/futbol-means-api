{
  "name": "means-api",
  "version": "1.0.0",
  "title": "Means API",
  "description": "An API allowing users to efficiently enter match data.",
  "main": "dist/server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "global": "npm i -g nodemon  rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:debug": "npm-run-all -s clean build -p watch:* debug",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "clean:swagger": "rm src/swagger.json",
    "debug": "node-debug",
    "build": "make dist",
    "build:old": "npm-run-all -s clean:dist build:swagger copy",
    "copy": "cp package.json dist/ && cp README.md dist/ && cp tsoa.json dist/",
    "build:swagger": "npm run swagger-gen && npm run routes-gen && npm run tsc && npm run copy-swagger-ui",
    "watch:tosa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect --delay 2.5 ./dist/server.js ",
    "watch:ts": "npm run tsc --watch",
    "lint": "tslint ./src/**/*.ts",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "copy-swagger-ui": "ncp ./src/presentation-layer/documentation/swagger-ui ./dist/swagger-ui",
    "tsoa": "tsoa",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "something"
  },
  "keywords": [
    "futbol",
    "means"
  ],
  "author": "Omar Crosby",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "class-validator": "^0.14.0",
    "codeclimate-test-reporter": "^0.5.1",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "event-dispatch": "^0.4.1",
    "express": "^4.18.2",
    "express-healthcheck": "^0.1.0",
    "express-ping": "^0.4.2",
    "express-winston": "^4.2.0",
    "glob": "^8.1.0",
    "http-errors": "^2.0.0",
    "istanbul": "^0.4.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "mongodb": "^3.7.3",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "onchange": "^7.1.0",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "remap-istanbul": "^0.13.0",
    "save-dev": "^0.0.1-security",
    "source-map-support": "^0.5.21",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^5.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/express-healthcheck": "^0.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.18.12",
    "@types/pug": "^2.0.6",
    "@types/shelljs": "^0.8.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "fs-extra": "^10.1.0",
    "jest": "^29.4.1",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "supertest": "^6.3.3",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-formatter": "^7.2.2"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npm run dev:start"
  }
}
