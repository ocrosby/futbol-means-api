{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*.controller.ts",
    "src/**/*Controller.ts"
  ],
  "spec": {
    "outputDirectory": "dist",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/api",
    "routesDir": "src/service-layer/controllers"
  }
}
